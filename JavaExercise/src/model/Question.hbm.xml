<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model">
<class name="Question" table="java_question">
<id name="questionid">
<generator class="native"/>
</id>
<property name="title"/>
<property name="selectionA"/>
<property name="selectionB"/>
<property name="selectionC"/>
<property name="selectionD"/>
<property name="right_answer"/>
<property name="question_type"/>
</class>
</hibernate-mapping>